# Detector Backend Configuration Examples

# ============================================
# OPTION 1: Mock Detector (default, fast testing)
# ============================================
DETECTOR_BACKEND=mock
DETECTION_CONFIDENCE_THRESHOLD=0.7
FRAME_EXTRACTION_FPS=2

# ============================================
# OPTION 2: Real YOLO + EasyOCR (ffmpeg-based, Replit-safe)
# ============================================
DETECTOR_BACKEND=yolo_ffmpeg
YOLO_MODEL=keremberke/yolov8n-license-plate
DETECT_CONFIDENCE=0.30
DETECTION_CONFIDENCE_THRESHOLD=0.30
FRAME_EXTRACTION_FPS=1
DEVICE=cpu  # or "cuda" if GPU available
MIN_BOX_WIDTH=20
MIN_BOX_HEIGHT=10

# ============================================
# OPTION 3: Real YOLO + EasyOCR (cv2-based, legacy)
# ============================================
# DETECTOR_BACKEND=yolo
# Note: May not work reliably in Replit due to cv2 issues
